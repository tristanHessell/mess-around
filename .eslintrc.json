{
  "env": {
    "es6": true
  },
  "extends": ["eslint:recommended","plugin:prettier/recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["warn", {"singleQuote": true, "trailingComma": "all", "arrowParens": "always"}],
    "no-unused-vars": ["error", { "varsIgnorePattern": "UNUSED"}],
    "react/prop-types": ["off"],
    "no-console": ["off"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
