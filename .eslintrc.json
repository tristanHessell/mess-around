{
  "env": {
    "es6": true
  },
  "extends": ["eslint:recommended","plugin:prettier/recommended", "plugin:react/recommended", "react-app"],
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true, "trailingComma": "all", "arrowParens": "always"}],
    "no-unused-vars": ["error", { "varsIgnorePattern": "UNUSED"}],
    "react/prop-types": ["off"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}